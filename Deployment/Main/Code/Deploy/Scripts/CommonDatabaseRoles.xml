      <TestInfo Ignore="false">
        <Sql>
          DECLARE @deploymentErrorCount INT
          SELECT @deploymentErrorCount=COUNT(1) FROM [MasterData_Logging].[dbo].[Deployment_Errors]

          DECLARE @errorLogCount INT
          SELECT @errorLogCount=COUNT(1) FROM [MasterData_Logging].[dbo].ELMAH_Error
          WHERE TimeUtc > DATEADD(MM, 10, GetDate())

          IF (@deploymentErrorCount >=1)
          BEGIN
          DECLARE @message Varchar(100)=CAST(@deploymentErrorCount as varchar(5)) + ' deployment error(s) logged in the Deployment_Errors table';
          THROW 50000, @message, 1;
          END

          IF (@errorLogCount >=1)
          BEGIN
          DECLARE @errorLogMessage Varchar(100)=CAST(@errorLogCount as varchar(5)) + ' errors logged in the Elmah_Error log table';
          THROW 50000, @errorLogMessage , 1;
          END
        </Sql>
