  <Target Name="BeforeBuild">
    <Message Text="Starting Before build" Importance="High" />
    <ItemGroup>
      <scriptFiles Include="$(MSBuildProjectDir)..\Scripts\**\*.*" />
      <helperScriptFiles Include="$(MSBuildProjectDir)..\HelperScripts\*.*" />
      <postDeployScriptFiles Include="$(MSBuildProjectDir)..\PostDeployScripts\*.*" />
      <sqlHelperScriptFiles Include="$(MSBuildProjectDir)..\HelperScripts\SQLHelpers\DeploymentHelpers\*.*" />
      <groupFiles Include="$(MSBuildProjectDir)..\Groups\*.*" />
      <accountFiles Include="$(MSBuildProjectDir)..\Accounts\*.*" />
      <parameterFiles Include="$(MSBuildProjectDir)..\Parameters\*.*" />
      <fileIncludeFiles Include="$(MSBuildProjectDir)..\FileInclude\*.*" />
      <toolFiles Include="$(MSBuildProjectDir)..\Tools\PSExec*.exe" />
      <wcfFiles Include="$(MSBuildProjectDir)..\Tools\WCFTestClient\*.*" />
      <softwareFiles Include="$(MSBuildProjectDir)..\Software\Common\WebDeploy_3_0_amd64_en-US.msi" />
      <DeploymentDlls Include="$(OutDir)\Deployment.*.dll" />
      <deploymentSchemaScripts Include="$(MSBuildProjectDir)..\..\Build\DeploymentSchema.Scripts\**\*.*" />
      <dynamicConfigFiles Include="$(MSBuildProjectDir)..\DynamicConfig\UniqueEnvironmentConfig\*.*" />
      <dynamicConfigMappings Include="$(MSBuildProjectDir)..\DynamicConfig\PlaceholderMappings\*.*" />
    </ItemGroup>
    <MakeDir Directories="$(OutDir)\DeploymentSchema.Scripts" />
    <Message Text="Copy Scripts directory to bin" Importance="High" />
    <Copy SourceFiles="@(scriptFiles)" DestinationFolder="$(OutDir)\Deployment\Scripts\%(RecursiveDir)" />
    <Copy SourceFiles="@(helperScriptFiles)" DestinationFolder="$(OutDir)\Deployment\HelperScripts" />
    <Copy SourceFiles="@(postDeployScriptFiles)" DestinationFolder="$(OutDir)\Deployment\PostDeployScripts" />
    <Copy SourceFiles="@(sqlHelperScriptFiles)" DestinationFolder="$(OutDir)\Deployment\HelperScripts\SQLHelpers\DeploymentHelpers" />
    <Copy SourceFiles="@(groupFiles)" DestinationFolder="$(OutDir)\Deployment\Groups" />
    <Copy SourceFiles="@(accountFiles)" DestinationFolder="$(OutDir)\Deployment\Accounts" />
    <Copy SourceFiles="@(parameterFiles)" DestinationFolder="$(OutDir)\Deployment\Parameters" />
    <Copy SourceFiles="@(fileIncludeFiles)" DestinationFolder="$(OutDir)\Deployment\FileInclude" />
    <Copy SourceFiles="@(toolFiles)" DestinationFolder="$(OutDir)\Deployment\Tools" />
    <Copy SourceFiles="@(wcfFiles)" DestinationFolder="$(OutDir)\Deployment\Tools\WCFTestClient" />
    <Copy SourceFiles="@(softwareFiles)" DestinationFolder="$(OutDir)\Deployment\Software\Common" />
    <Copy SourceFiles="@(DeploymentDlls)" DestinationFolder="$(OutDir)\Deployment\Scripts" />
    <Copy SourceFiles="@(deploymentSchemaScripts)" DestinationFolder="$(OutDir)\DeploymentSchema.Scripts\%(RecursiveDir)" />
    <Copy SourceFiles="@(dynamicConfigFiles)" DestinationFolder="$(OutDir)\Deployment\DynamicConfig\UniqueEnvironmentConfig\" />
    <Copy SourceFiles="@(dynamicConfigMappings)" DestinationFolder="$(OutDir)\Deployment\DynamicConfig\PlaceholderMappings\" />
